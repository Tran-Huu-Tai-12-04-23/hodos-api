{
  "name": "app",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "ts": "ts-node",
    "start": "NODE_ENV=development && expo start --dev-client",
    "local:ios": "NODE_ENV=development expo run:ios --scheme Hodos-Hackaton",
    "local:android": "NODE_ENV=development expo run:android ",
    "distribution:ios": "NODE_ENV=development eas build --profile development --platform ios",
    "distribution:android": "NODE_ENV=development eas build --profile development --platform android",
    "ota": " NODE_ENV=development eas update --branch development",
    "prebuild:ios": "NODE_ENV=production expo prebuild --platform ios",
    "prebuild:android": "NODE_ENV=development expo prebuild --platform android",
    "android": "NODE_ENV=production expo run:android",
    "ios": "NODE_ENV=production expo run:ios"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-voice/voice": "^3.2.4",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.4",
    "@shopify/flash-list": "^1.7.1",
    "@tanstack/react-query": "^5.51.1",
    "@types/react": "~18.2.79",
    "axios": "^1.7.2",
    "expo": "~51.0.28",
    "expo-blur": "~13.0.2",
    "expo-build-properties": "^0.12.5",
    "expo-dev-client": "~4.0.27",
    "expo-image-picker": "~15.0.7",
    "expo-location": "^17.0.1",
    "expo-secure-store": "^13.0.2",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.11.1",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-pager-view": "^6.4.1",
    "react-native-reanimated": "^3.15.3",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-svg": "^15.7.1",
    "react-native-uuid": "^2.0.2",
    "typescript": "~5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
